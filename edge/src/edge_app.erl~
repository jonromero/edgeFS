%%%-------------------------------------------------------------------
%% @doc edge public API
%% @end
%%%-------------------------------------------------------------------

-module('edge_app').

-behaviour(application).

%% Application callbacks
-export([start/2
        ,stop/1]).

%%====================================================================
%% API
%%====================================================================

start(_StartType, _StartArgs) ->
    'edge_sup':start_link().

%%--------------------------------------------------------------------
stop(_State) ->
    ok.

